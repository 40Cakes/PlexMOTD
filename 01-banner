#!/bin/bash

YEL='\033[0;33m'
PUR='\033[0;35m'
NC='\033[0m'


echo -e "
 @@@@@@@@@@@@@     @@@@@            @@@@@@@@@@@@@   ${YEL}///////${NC}     #@@@@@@
 @@@@@@@@@@@@@@@   @@@@@            @@@@@@@@@@@@@    ${YEL}////(((${NC}   @@@@@@@
 @@@@@     @@@@@@  @@@@@            @@@@#             ${YEL}//(((((${NC} @@@@@@@
 @@@@@      @@@@@  @@@@@            @@@@#               ${YEL}((((((${NC}&@@@@&
 @@@@@     %@@@@@  @@@@@            @@@@@@@@@@@@         ${YEL}(((((((${NC}@@(
 @@@@@@@@@@@@@@@   @@@@@            @@@@@@@@@@@@          ${YEL}(((((((${NC}
 @@@@@@@@@@@@@     @@@@@            @@@@#                ${YEL}/((((((${NC}@@
 @@@@@             @@@@@            @@@@#               ${YEL}(((((((${NC}@@@@
 @@@@@             @@@@@            @@@@#              ${YEL}/(((((/${NC}@@@@@@#
 @@@@@             @@@@@@@@@@@@@@   @@@@@@@@@@@@@    ${YEL}*///(((${NC}   @@@@@@@
 @@@@@             @@@@@@@@@@@@@@   @@@@@@@@@@@@@   ${YEL}//////(${NC}     @@@@@@@
"
echo " "
echo -e " ${YEL}CPU.................:${NC} ${PUR}$(lscpu | grep -oP 'Model name:\s*\K.+ | xargs ')${NC}"
